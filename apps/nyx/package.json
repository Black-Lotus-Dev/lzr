{
	"name": "nyx",
	"version": "1.0.0",
	"scripts": {
		"lint": "eslint --ext .js,.ts .",
		"build": "tsc",
		"build:watch": "tsc --watch",
		"serve": "pnpm run build && pnpm run kill && firebase emulators:start --only functions --inspect-functions",
		"shell": "pnpm run build && firebase functions:shell",
		"start": "pnpm run shell",
		"deploy": "firebase deploy --only functions",
		"logs": "firebase functions:log",
		"kill": "npx kill-port 5000 5001"
	},
	"engines": {
		"node": "16"
	},
	"main": "lib/index.js",
	"dependencies": {
		"@types/body-parser": "^1.19.2",
		"axios": "^1.2.0",
		"body-parser": "^1.20.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"firebase-admin": "^10.0.2",
		"firebase-functions": "^4.2.1",
		"lodash": "^4.17.21",
		"node-fetch": "^3.3.0",
		"nodemon": "^2.0.20",
		"request-promise": "^4.2.6",
		"socket.io": "^4.5.4",
		"spotify-api": "^0.0.0",
		"spotify-api.js": "^9.2.5",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@types/express": "^4.17.1",
