{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@api/*": ["./api/*"],
      "@assets/*": ["./assets/*"],
      "@components/*": ["./components/*"],
      "@configs/*": ["./configs/*"],
      "@constants/*": ["./constants/*"],
      "@lzrClients/*": ["./lzrClients/*"],
      "@lzrTypes/*": ["./types/*"],
      "@pages/*": ["./pages/*"],
      "@provider/*": ["./provider/*"],
      "@redux/*": ["./redux/*"],
      "@styles/*": ["./styles/*"],
      "@utils/*": ["./utils/*"]
    }
  },
  "include": ["src", "../lzrStreamApp/packages/lzrStore/types/music"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
